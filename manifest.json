{
  "manifest_version": 3,
  "name": "Andt Test Extension",
  "version": "1.0.0",
  "description": "A basic example extension",
  "icons": {
    "48": "images/icon.png",
    "128": "images/icon.png"
  },
  "action": {
    "default_title": "Click Me",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "injected.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}